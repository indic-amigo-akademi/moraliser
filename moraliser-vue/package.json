{
  "name": "moraliser",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite --port 5454",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 5454",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:e2e": "start-server-and-test preview :5454 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 5454' :5454 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@iconify/vue": "^4.3.0",
    "axios": "^1.8.3",
    "bootstrap": "^5.3.3",
    "moment": "^2.29.4",
    "vue": "^3.4.0",
    "vue-router": "^4.1.6",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.12",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "cypress": "^14.2.0",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "start-server-and-test": "^2.0.11",
    "typescript": "^5.8.2",
    "vite": "^6.2.2",
    "vitest": "^3.0.9",
    "vue-tsc": "^2.0.28"
  }
}
